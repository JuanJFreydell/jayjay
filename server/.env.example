# Calendly API Configuration
CALENDLY_API_KEY=your_personal_access_token_here
CALENDLY_API_BASE_URL=https://api.calendly.com

# Your Calendly user URI (format: https://api.calendly.com/users/XXXXXX)
CALENDLY_USER_URI=https://api.calendly.com/users/your_user_id

# Default event type URI for property tours (format: https://api.calendly.com/event_types/XXXXXX)
# You can map different properties to different event types if needed
CALENDLY_DEFAULT_EVENT_TYPE_URI=https://api.calendly.com/event_types/your_event_type_id

# Milvus Vector Database Configuration
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_COLLECTION_NAME=property_documents

# Embedding Model Configuration
EMBEDDING_MODEL=all-MiniLM-L6-v2
# Options: all-MiniLM-L6-v2 (fast, 384 dim), all-mpnet-base-v2 (better quality, 768 dim)

# Offer Database Configuration
OFFERS_DB_PATH=./data/offers.db
# Path to SQLite database for storing property offers

# REST API Server Configuration (for server.py)
SERVER_HOST=0.0.0.0
SERVER_PORT=8000

# SSL/HTTPS Configuration (optional, for production)
# Generate certificates with: openssl req -x509 -newkey rsa:4096 -keyout key.pem -out cert.pem -days 365 -nodes
SSL_KEYFILE=
SSL_CERTFILE=
# Leave empty for HTTP-only development mode
# Set paths to .pem files for HTTPS production deployment
